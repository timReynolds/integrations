{
  "name": "@ory/integrations",
  "version": "v0.0.1",
  "description": "Integrations for Ory Cloud",
  "author": "Ory GmbH",
  "prettier": "ory-prettier-styles",
  "license": "Apache-2.0",
  "repository": "https://github.com/ory/integrations",
  "config": {
    "prettierTarget": "**/*.{tsx,ts,json,md,js,css}"
  },
  "scripts": {
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "build": "rm -rf dist ui nextjs && rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest --detectOpenHandles"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/request": "^2.48.8",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.3.1",
    "esbuild": "^0.17.4",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "rollup": "^3.10.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4",
    "next": ">=12.0.10"
  },
  "peerDependencies": {
    "next": ">=12.0.10"
  },
  "dependencies": {
    "@ory/client": "^1.1.5",
    "cookie": "^0.5.0",
    "istextorbinary": "^6.0.0",
    "@types/tldjs": "^2.3.1",
    "tldjs": "^2.3.1",
    "ory-prettier-styles": "^1.3.0",
    "prettier": "^2.8.3",
    "request": "^2.88.2",
    "set-cookie-parser": "^2.5.1"
  }
}
